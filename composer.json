{
  "name": "tajawal/codeception-appium",
  "description": "Codeception appium driver model",
  "keywords": [
    "BDD",
    "acceptance testing",
    "functional testing",
    "unit testing",
    "tdd",
    "appium",
    "codeception"
  ],
  "type": "library",
  "license": "MIT",
  "version": "1.1.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "codeception/codeception": "~2",
    "phpunit/phpunit-selenium": "~4",
    "symfony/console": "~4"
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "overtrue/phplint": "~1",
    "squizlabs/php_codesniffer": "~3"
  },
  "autoload": {
    "psr-4": {
      "Appium\\": "src/",
      "Parser\\": "misc/Parser/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Appium\\Tests\\": "tests/"
    }
  }
}
